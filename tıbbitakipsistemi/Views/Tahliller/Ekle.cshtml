
@{
    ViewBag.Title = "Ekle";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using (Html.BeginForm("Ekle", "Tahliller", FormMethod.Post))
{

    <div class="container">
        <div class="row">
            <div class="col-md-6">

                <div class="form-group">
                    <label>Kullanici Id</label>
                    <input type="text" name="KullaniciId" class="form-control" />

                </div>
                <div class="form-group">
                    <label>Tarih</label>
                    <input type="datetime" name="Tarih" class="form-control" />

                </div>

                <div class="form-group">
                    <label> Tahlil</label>
                    <input type="text" name="TahlilBilgisi" class="form-control" />

                </div>
                <div class="form-group">
                    <label>Tahlil Sonucu</label>
                    <input type="text" name="TahlilSonuc" class="form-control" />

                </div>
                <button class="btn btn-primary" type="submit">Ekle</button>
                <a href="/Alerji/Index"></a>

            </div>


        </div>

    </div>
}